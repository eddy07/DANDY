<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true">
    <shape>
      <solid
          android:color="@color/app_color_light" />
       <corners
           android:radius="30dp"/>
    </shape>
  </item>
  <item>
    <shape>
      
      <solid
          android:color="@color/app_color" />
        <stroke
            android:color="@color/app_color"
            android:width="2dp"/>
       <corners 
           android:radius="30dp"/>
    </shape>
  </item>
    

</selector>
